/*

Question Link: https://codeforces.com/contest/1883/problem/B

*/

#include<bits/stdc++.h>
using namespace std;

#define ll long long int

void SIR(){
    ll n,k;
    cin>>n>>k;
    string s;
    cin>>s;
    ll a[26]={0};
    for(int i=0;i<n;i++){
        ll val=s[i]-'a';
        a[val]++;
    }
    ll odd=0;
    for(int i=0;i<26;i++){
        if(a[i]%2)
            odd++;
    }
    if(k==odd)
        cout<<"YES"<<endl;
    else if(k<odd){
        if(odd-k==1)
            cout<<"YES"<<endl;
        else
            cout<<"NO"<<endl;
    }
    else{
        k-=odd;
        if(k>n-odd)
            cout<<"NO"<<endl;
        else
            cout<<"YES"<<endl;
    }
}

int main(){
    ll tc;
    tc = 1;
    cin>>tc;
    while(tc--){
        SIR();
    }
    return 0;
}
