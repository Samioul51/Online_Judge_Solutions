/*

Problem Link: https://codeforces.com/contest/1165/submission/315049254

*/

#include<bits/stdc++.h>
using namespace std;

#define ll long long int

void solve(){
    ll n,i;
    cin>>n;
    multiset<ll> s;
    for(i=0;i<n;i++){
        ll val;
        cin>>val;
        s.insert(val);
    }
    ll k=1;
    for(auto x: s){
        if(x>=k)
            k++;
    }
    cout<<k-1<<endl;
}

int main(){
    ll tc;
    tc = 1;
    //cin>>tc;
    while(tc--){
        solve();
    }
    return 0;
}
