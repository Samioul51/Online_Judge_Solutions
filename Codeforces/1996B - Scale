/*

Problem Link: https://codeforces.com/contest/1996/problem/B

*/

#include<bits/stdc++.h>
using namespace std;

#define ll long long int

void solve(){
    ll n,k,i,j;
    cin>>n>>k;
    vector<string> v(n);
    for(i=0;i<n;i++)cin>>v[i];
    for(i=0;i<n;i+=k){
        for(j=0;j<n;j+=k){
            cout<<v[i][j];
        }
        cout<<"\n";
    }
}

int main(){
    ll tc;
    tc = 1;
    cin>>tc;
    while(tc--){
        solve();
    }
    return 0;
}
