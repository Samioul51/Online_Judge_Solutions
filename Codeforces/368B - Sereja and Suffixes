/*

Problem Link: https://codeforces.com/contest/368/problem/B

*/

#include<bits/stdc++.h>
using namespace std;

#define ll long long int

void solve(){
    ll n,m,i;
    cin>>n>>m;
    vector<ll> v(n+1);
    set<ll>s;
    vector<ll> res(n+1,0);
    for(i=1;i<=n;i++)cin>>v[i];
    for(i=n;i>=1;i--){
        if(i==n){
            res[i]=1;
            s.insert(v[i]);
            continue;
        }
        if(!s.count(v[i])){
            res[i]=res[i+1]+1;
            s.insert(v[i]);
        }
        else
            res[i]=res[i+1];
    }
    while(m--){
        ll q;
        cin>>q;
        cout<<res[q]<<endl;
    }
}

int main(){
    ll tc;
    tc = 1;
    //cin>>tc;
    while(tc--){
        solve();
    }
    return 0;
}
