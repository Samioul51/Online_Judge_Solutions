#include<bits/stdc++.h>
using namespace std;

#define ll long long int

void solve(){
    ll n,i;
    cin>>n;
    string s;
    cin>>s;
    if(s[0]!=s.back())
        cout<<"YES"<<endl;
    else
        cout<<"NO"<<endl;
}

int main(){
    ll tc;
    tc = 1;
    cin>>tc;
    while(tc--){
        solve();
    }
    return 0;
}
