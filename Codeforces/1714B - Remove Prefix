/*

Problem Link: https://codeforces.com/contest/1714/problem/B

*/

#include<bits/stdc++.h>
using namespace std;

#define ll long long int

void solve(){
    ll n,i,count=0,res=0;
    cin>>n;
    vector<ll> v(n);
    map<ll,ll> m;
    for(i=0;i<n;i++){
        cin>>v[i];
        m[v[i]]++;
    }
    for(auto it=m.begin();it!=m.end();it++){
        ll rmv=it->second-1;
        count+=rmv;        
    }
    for(i=0;i<n;i++){
        if(count==0)
            break;
        if(m[v[i]]>1){
            count--;
            m[v[i]]--;
        }
        res++;
    }
    cout<<res<<endl;
}

int main(){
    ll tc;
    tc = 1;
    cin>>tc;
    while(tc--){
        solve();
    }
    return 0;
}
