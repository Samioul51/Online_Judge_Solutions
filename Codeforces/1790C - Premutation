/*

Problem Link: https://codeforces.com/contest/1790/problem/C

*/

#include<bits/stdc++.h>
using namespace std;

#define ll long long int

void solve(){
    ll n,i,j;
    cin>>n;
    vector<vector<ll>> v(n,vector<ll>(n-1));
    for(i=0;i<n;i++){
        for(j=0;j<n-1;j++){
            cin>>v[i][j];
        }
    }
    map<ll,ll>m;
    for(i=0;i<n;i++){
        m[v[i][0]]++;
    }
    ll maxx=m[v[0][0]];
    ll ele=v[0][0];
    vector<ll> res;
    for(auto it=m.begin();it!=m.end();it++){
        if(maxx<it->second){
            ele=it->first;
            maxx=it->second;
        }
    }
    res.push_back(ele);
    for(auto it=m.begin();it!=m.end();it++){
        if(ele!=it->first){
            res.push_back(it->first);
            break;
        }
    }
    for(i=1;i<n-1;i++){
        for(j=0;j<n;j++){
            if(v[j][i]!=res.back()){
                res.push_back(v[j][i]);
                break;
            }
        }
    }
    for(auto x:res){
        cout<<x<<" ";
    }
    cout<<endl;
}

int main(){
    ll tc;
    tc = 1;
    cin>>tc;
    while(tc--){
        solve();
    }
    return 0;
}
