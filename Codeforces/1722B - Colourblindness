/*

Question Link: https://codeforces.com/contest/1722/problem/B

*/

#include<bits/stdc++.h>
using namespace std;

#define ll long long int

void SIR(){
    ll n;
    cin>>n;
    string a,b;
    cin>>a>>b;
    for(int i=0;i<n;i++){
        if(a[i]!=b[i]){
            if((a[i]=='G' and b[i]=='B') or (a[i]=='B' and b[i]=='G'))
                continue;
            else{
                cout<<"NO"<<endl;
                return;
            }
        }
    }
    cout<<"YES"<<endl;
}

int main(){
    ll tc;
    tc = 1;
    cin>>tc;
    while(tc--){
        SIR();
    }
    return 0;
}
